{
    "name": "@misiki/www",
    "version": "1.0.0",
    "description": "Misiki Reloaded",
    "author": "Swadesh Behera",
    "private": true,
    "config": {
        "nuxt": {
            "host": "0.0.0.0",
            "port": "8888"
        }
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development nuxt",
        "start": "cross-env NODE_ENV=production nuxt start",
        "prod": "run-s build copyNetlify",
        "copyNetlify": "node devops/copy-netlify.js",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf public/build && npm install && npm run build:tailwind && npm run build:fontawesome && npm run dev",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "precommit": "npm run lint",
        "copy": "node devops/copy.js",
        "live": "run-s build copy liveS",
        "liveS": "node devops/live.js",
        "generate": "nuxt generate",
        "build": "cross-env NODE_ENV=production nuxt build",
        "analyze": "nuxt build -a",
        "dist": "http-server dist -P http://localhost:6600"
    },
